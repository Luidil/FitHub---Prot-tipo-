{
  "appId": "com.fithub.app",
  "appName": "FitHub",
  "webDir": "dist",
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#03050b",
      "showSpinner": false,
      "androidSpinnerStyle": "small",
      "spinnerColor": "#19f18f"
    },
    "StatusBar": {
      "style": "Dark",
      "backgroundColor": "#03050b"
    }
  },
  "android": {
    "allowMixedContent": true
  },
  "ios": {
    "contentInset": "automatic"
  }
}
